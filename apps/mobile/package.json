{
  "name": "@bet-think/mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "generate:api": "openapi-typescript-codegen --input ../../services/api/openapi.yaml --output ./src/api/generated --client axios",
    "prebuild": "expo prebuild",
    "build:dev": "eas build --profile development",
    "build:staging": "eas build --profile staging",
    "build:prod": "eas build --profile production",
    "update:dev": "eas update --branch development --message",
    "update:staging": "eas update --branch staging --message",
    "update:prod": "eas update --branch production --message"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.4.7",
    "@betthink/shared": "npm:@jp-rockpile/shared@0.2.0",
    "@expo/metro-runtime": "~6.1.2",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@sentry/react-native": "~7.2.0",
    "@tanstack/react-query": "^5.28.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "expo": "~54.0.13",
    "expo-auth-session": "^7.0.8",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-device": "~8.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-auth0": "^3.2.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-uuid": "^2.0.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-worklets": "0.5.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.5",
    "@types/react": "~19.1.17",
    "@types/react-native": "^0.73.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.18.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.12",
    "openapi-typescript-codegen": "^0.27.0",
    "prettier": "^3.2.5",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/api/generated/**"
    ]
  },
  "private": true
}
