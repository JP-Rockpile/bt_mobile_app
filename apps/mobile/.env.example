# App Environment
APP_ENV=development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_DOMAIN=betthink.app

# Auth0 Configuration
EXPO_PUBLIC_AUTH0_DOMAIN=your-tenant.auth0.com
EXPO_PUBLIC_AUTH0_CLIENT_ID=your-client-id
EXPO_PUBLIC_AUTH0_AUDIENCE=https://api.betthink.app

# Analytics
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-amplitude-key

# Error Tracking
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ORG=your-org
SENTRY_PROJECT=bet-think-mobile
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# EAS
EAS_PROJECT_ID=your-eas-project-id

# Feature Flags
EXPO_PUBLIC_ENABLE_DEV_MENU=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false
