{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "build:dev": "eas build --profile development --platform all",
    "build:staging": "eas build --profile staging --platform all",
    "build:prod": "eas build --profile production --platform all",
    "update": "eas update",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "clean": "rm -rf node_modules .expo dist",
    "prebuild": "expo prebuild",
    "generate:api": "openapi-typescript-codegen --input ./openapi.yaml --output ./src/services/generated --client fetch"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.5.13",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@sentry/react-native": "^7.2.0",
    "@tamagui/animations-react-native": "^1.135.0",
    "@tamagui/config": "^1.135.0",
    "@tamagui/core": "^1.135.0",
    "@tanstack/react-query": "^5.90.2",
    "date-fns": "^4.1.0",
    "eventsource-parser": "^3.0.6",
    "expo": "~54.0.10",
    "expo-auth-session": "^7.0.8",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "^18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-device": "^8.0.8",
    "expo-linking": "^8.0.8",
    "expo-notifications": "^0.32.11",
    "expo-router": "^6.0.8",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "^29.0.11",
    "expo-web-browser": "^15.0.7",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-bottom-sheet": "^1.0.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@config-plugins/detox": "^11.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.42.0",
    "eslint": "^9.36.0",
    "eslint-config-expo": "^10.0.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.12",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
